        <Label x:Name="label_ugovori" Content="Izaberite ugovor koji zelite obrisati:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Foreground="GreenYellow"/>
        <ListBox x:Name="listbox_ugovori" HorizontalAlignment="Left" Height="144" Margin="10,41,0,0" VerticalAlignment="Top" Width="272" TabIndex="1"/>
        <Button x:Name="button_cancel" Content="Cancel" HorizontalAlignment="Left" Margin="207,203,0,0" VerticalAlignment="Top" Width="75" BorderBrush="#FF66764E" Background="GreenYellow" Click="button_cancel_Click" TabIndex="3"/>
        <Button x:Name="button_obrisi1" Content="Obrisi ugovor" HorizontalAlignment="Left" Margin="101,203,0,0" VerticalAlignment="Top" Width="88" BorderBrush="#FF66764E" Background="GreenYellow" Click="button_obrisi1_Click" TabIndex="2"/>

    </Grid>
</Window>
